---
layout: nil
base_path: http://potomak.github.io/techpeaks
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>TechPeaks Log</title>
  <link href="{{ page.base_path }}/atom.xml" rel="self"/>
  <link href="{{ page.base_path }}"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ page.base_path }}</id>
  <author>
    <name>TechPeaks Log</name>
    <email>potomak84@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="{{ page.base_path }}{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ page.base_path }}{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>